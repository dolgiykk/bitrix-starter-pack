{
  "autoload": {
    "psr-4": {
      "App\\": "local/App"
    }
  },
  "extra": {
    "installer-paths": {
      "local/modules/{$name}/": ["type:bitrix-module"]
    }
  },
  "require": {
    "andreyryabin/sprint.migration": "dev-master"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^v3.0.0"
  },
  "scripts": {
    "cs-fix": "php-cs-fixer fix"
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true
    }
  }
}
